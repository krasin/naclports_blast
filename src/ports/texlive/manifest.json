{
  "name": "TeX Live",
  "description": "TeX Live is an easy way to get up and running with the TeX document production system.",
  "version": "2013.5.30",
  "manifest_version": 2,
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "storage",
    "unlimitedStorage"
  ],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  }
}
